{
    "name": "aymdev/fregata",
    "description": "Database migration framework allowing data migration between different DBMS or database structures.",
    "type": "framework",
    "keywords": [
        "database",
        "doctrine",
        "migration",
        "migration-framework",
        "migration-tool",
        "sql"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "AymDev",
            "email": "aymericmayeux@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4",
        "doctrine/dbal": "^2.10",
        "symfony/console": "^5.0",
        "symfony/yaml": "^5.0",
        "php-di/php-di": "^6.1",
        "symfony/config": "^5.2",
        "symfony/dependency-injection": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Fregata\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fregata\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "mikey179/vfsstream": "^1.6"
    },
    "bin": ["bin/fregata"],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
